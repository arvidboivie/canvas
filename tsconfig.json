{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2023", "DOM"],
    // "experimentalDecorators": true,
    // "emitDecoratorMetadata": true,

    "module": "ESNext",
    "moduleResolution": "Bundler",
    "sourceMap": true,
    "verbatimModuleSyntax": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
